{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/index.js" },
    { "src": "/health", "dest": "/index.js" },
    { "src": "/auth/(.*)", "dest": "/index.js" },
    { "src": "/movies/(.*)", "dest": "/index.js" },
    { "src": "/funtionCine/(.*)", "dest": "/index.js" },
    { "src": "/reservaMovie/(.*)", "dest": "/index.js" },
    { "src": "/upload/(.*)", "dest": "/index.js" }
  ]
}
